	<jms:config name="JMS_Config" doc:name="JMS Config" doc:id="e89735f1-1936-42e9-82d1-1f5458a956b7" >
		<jms:active-mq-connection username="${jms.settings.user}" password="${jms.settings.passwd}">
			<reconnection >
				<reconnect-forever frequency="${jms.reconnection.frequency.ms}" />
			</reconnection>
			<jms:factory-configuration brokerUrl="${jms.settings.url}" />
		</jms:active-mq-connection>
		<jms:producer-config timeToLive="3" timeToLiveUnit="DAYS" />
	</jms:config>	
		<ftp:config name="FTP_Config_effem" doc:name="FTP Config" doc:id="71b0857a-1a7e-4617-b58a-cc1e1828b86d" >
		<ftp:connection host="${sftp.host}" port="${sftp.port}" username="${sftp.username}" password="${sftp.password}" connectionTimeout="${sftp.connection.timeout.seconds}" responseTimeout="${sftp.response.timeout.seconds}">
			<reconnection >
				<reconnect frequency="${sftp.reconnection.strategy.frequency.ms}" count="${sftp.reconnection.strategy.reconnection.attempts.ms}" />
			</reconnection>
		</ftp:connection>
	</ftp:config>
	<http:request-config name="Http_request_system-tms-findentities" doc:name="HTTP Request configuration" doc:id="461e8e37-74e6-4b6a-bcab-3e2f5f74739c" >
