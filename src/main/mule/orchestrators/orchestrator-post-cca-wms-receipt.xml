			<ee:message >
			</ee:message>
			<ee:variables >
				<ee:set-variable variableName="requestLogin" ><![CDATA[%dw 2.0
output application/json
---
{
	"usr_id": p('client.wms.blueyonder.https.path.login.user'),
	"password": p('client.wms.blueyonder.https.path.login.password')
}]]></ee:set-variable>
			</ee:variables>
		</ee:transform>
		<flow-ref doc:name="Call-client-wms-blueyonder-login" doc:id="5b668ad3-6c67-4dc1-8ae1-f5c9c2eb20ca" name="client-wms-blueyonder-login"/>
