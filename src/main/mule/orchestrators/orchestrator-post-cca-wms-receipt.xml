		<ee:transform doc:name="Back up payload" doc:id="4638d8f3-49ba-474c-a44a-864492b6861a" >
			<ee:message >
			</ee:message>
			<ee:variables >
				<ee:set-variable variableName="backUpPayload" ><![CDATA[%dw 2.0
output application/xml
---
payload]]></ee:set-variable>
			</ee:variables>
		</ee:transform>
